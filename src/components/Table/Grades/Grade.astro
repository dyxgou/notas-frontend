---
interface Props {
  gradeId: number;
  grade: string;
}

const { grade, gradeId } = Astro.props;

console.log({ grade, gradeId });
---

<th class="table-header">
  <input type="hidden" value={gradeId} />

  <input
    class="w-5/6 text-center font-semibold text-slate-800 border-b-2 border-r text-sm whitespace-nowrap relative bg-white/25 backdrop-blur-sm border border-white/20"
    type="text"
    maxlength="15"
    value={grade}
  />
</th>
